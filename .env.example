VITE_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
VITE_SUPABASE_ANON_KEY=YOUR_ANON_KEY

# Mapbox public token (starts with pk.)
VITE_MAPBOX_TOKEN=YOUR_MAPBOX_PUBLIC_TOKEN
# Optional: force a specific map engine
# VITE_FORCE_MAPLIBRE=true
# VITE_MAP_ENGINE=mapbox

# Google Maps Platform (for geocoding/autocomplete in Quest Builder)
# Enable Geocoding API and Places API in your GCP project
VITE_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY