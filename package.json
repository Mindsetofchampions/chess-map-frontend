{
  "name": "chess-map-frontend",
  "private": true,
  "version": "0.1.0",
  "description": "CHESS Map front-end plugin with glass-morphic UI and Mapbox integration",
  "type": "module",
  "scripts": {
    "start:frontend": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix --max-warnings 0",
    "lint:check": "eslint 'src/**/*.{js,jsx,ts,tsx}' --max-warnings 0",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,md}' && prettier --write '*.{js,ts,json,md}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css,md}' && prettier --check '*.{js,ts,json,md}'",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "clean": "rm -rf dist",
    "quality": "npm run type-check && npm run lint:check && npm run format:check",
    "quality:fix": "npm run type-check && npm run lint && npm run format",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --project-token=$CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes",
    "pre-commit": "npm run quality:fix && npm run test",
    "pre-push": "npm run quality && npm run test:coverage"
  },
  "dependencies": {
    "@supabase/auth-ui-react": "^0.4.6",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.39.0",
    "@types/mapbox-gl": "^3.4.1",
    "concurrently": "^8.0.0",
    "framer-motion": "^12.23.11",
    "lucide-react": "^0.292.0",
    "mapbox-gl": "^3.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@storybook/addon-a11y": "^7.6.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/addon-storysource": "^7.6.0",
    "@storybook/blocks": "^7.6.0",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "babel-jest": "^30.0.5",
    "chromatic": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "jest-transform-stub": "^2.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "storybook": "^7.6.0",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.0",
    "typescript": "^5.5.3",
    "vite": "^5.4.2"
  }
}
